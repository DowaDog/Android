<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_marginTop="25dp"
        android:id="@+id/layout_commynity_item_profile_frame"
        android:layout_width="match_parent"
        android:layout_height="32.4dp">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img_community_item_profile"
            android:layout_width="32.4dp"
            android:layout_height="match_parent"

            />
        <TextView
            android:id="@+id/tv_community_item_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="aaaaaaaaa"
            android:textSize="14sp"
            android:fontFamily="@font/nanum_bold"
            android:textColor="#434343"
            android:includeFontPadding="false"
            android:layout_toRightOf="@+id/img_community_item_profile"
            android:layout_marginStart="9dp"
            />

        <TextView
            android:id="@+id/tv_community_item_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6시간 전"
            android:layout_centerVertical="true"
            android:textColor="#a6a6a6"
            android:textSize="11sp"
            android:layout_alignParentRight="true"
            android:fontFamily="@font/nanum_round"
            />
        <ImageView
            android:id="@+id/btn_community_item_more"
            android:src="@drawable/back_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:visibility="gone"
            />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/layout_commynity_item_image_frame"
        android:layout_below="@+id/layout_commynity_item_profile_frame"
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:layout_height="210dp">
        <ImageView
            android:id="@+id/img_community_item_main1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"
            android:src="#232323"
            />
        <LinearLayout
            android:id="@+id/img_community_item_main2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/img_community_item_main21"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scaleType="fitXY"
                android:layout_marginRight="5dp"
                />
            <ImageView
                android:id="@+id/img_community_item_main22"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scaleType="fitXY"
                android:layout_marginLeft="5dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/img_community_item_main3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/img_community_item_main31"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:scaleType="fitXY"
                android:layout_marginRight="5dp"
                />
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_marginLeft="5dp"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/img_community_item_main32"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:layout_marginBottom="5dp"
                    />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    >
                <ImageView
                    android:id="@+id/img_community_item_main33"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    />

                    <RelativeLayout
                        android:id="@+id/img_community_item_main4"
                        android:background="#66000000"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:fontFamily="@font/nanum_bold"
                        android:textColor="#ffffff"
                        android:layout_centerInParent="true"
                        android:text="+1"
                        />
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>


    <TextView
        android:layout_below="@+id/layout_commynity_item_image_frame"
        android:id="@+id/tv_community_item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:fontFamily="@font/nanum_extra_bold"
        android:textSize="18sp"
        android:textColor="#434343"
        android:text="title"
        />

</RelativeLayout>